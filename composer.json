{
    "name": "yepos/keyword",
    "description": "Record keyword for everyday.",
    "type": "library",
    "license": "MIT",
    "author": [
        {
            "name": "Kevin Chen",
            "email": "uncochen@hotmail.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Yepos\\Keyword\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yepos\\Keyword\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Yepos\\Keyword\\KeywordServiceProvider"
            ]
        }
    },
    "require-dev": {
        "orchestra/testbench": "8.0.x-dev"
    },
    "scripts": {
        "post-autoload-dump": [
            "@php ./vendor/bin/testbench package:discover --ansi"
        ]
    }
}
